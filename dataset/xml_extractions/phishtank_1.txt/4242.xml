b'\r\n        <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">\r\n        <html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">\r\n        <head>\r\n        <title>AliveChat Session</title>\r\n        <link href="https://fonts.googleapis.com/css?family=Nunito:200,300,400,600,700" rel="stylesheet" />\r\n\t\t<style type="text/css">\r\n\t<!--\r\n\t/* ---------------------------------------------\r\n\t General Styles\r\n\t---------------------------------------------*/\r\n\tdiv.general_text{color:#333333; text-decoration:none; font-family:Arial; font-size:14px;}\r\n\tspan.general_text{color:#333333; text-decoration:none; font-family:Arial; font-size:14px;}\r\n\ta.general_text{color:#333333; text-decoration:underline; font-family:Arial; font-size:14px;}\r\n\ta.general_text:visited{color:#333333; text-decoration:underline; font-family:Arial; font-size:14px;}\r\n\ta.general_text:active{color:#333333; text-decoration:underline; font-family:Arial; font-size:14px;}\r\n\ta.general_text:hover{color:#6C9CF3; text-decoration:underline; font-family:Arial; font-size:14px;}\r\n\t\r\n\t/* ---------------------------------------------\r\n\t General Input Styles\r\n\t---------------------------------------------*/\r\n\t\r\n\t\r\n\t\r\n\tinput.general_submit{\r\n\t\tcolor: #000000; \r\n\t\ttext-decoration: none; \r\n\t\tfont-family: Arial; \r\n\t\tfont-size: 14px; \r\n\t\tbackground-color: #CCCCCC;\r\n\t\tborder:0px;\r\n\t\tmargin: 0px;\r\n\t}\r\n\t\r\n\tinput.general_textbox{\r\n\r\n\t\tcolor: #000000; \r\n\t\ttext-decoration: none; \r\n\t\tfont-family: Arial; \r\n\t\tfont-size: 14px; \r\n\t\tbackground-color: #FFFFFF; \r\n\t\t/*\r\n\t\tborder: 2px #CCCCCC solid; \r\n\t\t*/\r\n\t\tborder:0px;\r\n\t\tpadding:2px;\r\n\t\tmargin-top:0px;\r\n\t\tmargin-bottom:0px;\r\n\r\n\t}\r\n\tselect.general_select{\r\n\t\t\r\n\t\tcolor: #000000; \r\n\t\ttext-decoration: none; \r\n\t\tfont-family: Arial; \r\n\t\tfont-size: 14px; \r\n\t\tbackground-color: #FFFFFF; \r\n\t\t/*\r\n\t\tborder: 2px #CCCCCC solid;\r\n\t\t*/\r\n\t\tpadding:2px;\r\n\t\tborder:0px;\r\n\t\theight:30px;\r\n\t}\r\n\t\r\n\ttextarea.general_textarea{\r\n\t\tcolor: #000000; \r\n\t\ttext-decoration: none; \r\n\t\tfont-family: Arial; \r\n\t\tfont-size: 14px; \r\n\t\tbackground-color: #FFFFFF; \r\n\t\theight:50px; \r\n\t\t/*\r\n\t\tborder: 2px #CCCCCC solid; \r\n\t\t*/\r\n\t\tpadding:4px;\r\n\t\tmargin-top:0px;\r\n\t\tmargin-bottom:0px;\r\n\t\tborder:0px;\r\n\t\twidth:100%;\r\n\t\t\r\n\t}\r\n\t\r\n\t-->\r\n\t\r\n\t\r\n\t.formwrap{\r\n\t\r\n\t\tfloat:left;\r\n\t\tpadding-left:2px;\r\n\t\tpadding-right:2px;\r\n\t\tpadding-top:2px;\r\n\t\tpadding-bottom:2px;\r\n\t\t\r\n\t\tbackground:#CCCCCC;\r\n\t\tmargin-top:5px;\r\n\t\r\n\t}\r\n\t\r\n\t.formwrap input {\r\n\t\r\n\t\tmargin: 0px;\r\n\t\tpadding:6px;\r\n\t\twidth:225px;\r\n\t\t\r\n\t\t\r\n\t\r\n\t}\r\n\t\r\n\t\r\n\t\r\n\t.formwrap select {\r\n\t\r\n\t\tmargin: 0px;\r\n\t\tpadding:6px;\r\n\t\twidth:237px;\r\n\t}\r\n\t\r\n\t.formwrap textarea {\r\n\t\r\n\t\tmargin: 0px;\r\n\t\tpadding:6px;\r\n\t\twidth:225px;\r\n\t\t\r\n\t\t/*width:418px;*/\r\n\t\t\r\n\t\theight:50px; /* this makes the form textarea with a set height \xe2\x80\x93 remove if you do not want */\r\n\t\tresize:none; /* be sure to put resize = none on all textarea boxes, this prevents people from messing up the layout by dragging the box to a larger than normal size */\r\n\t\r\n\t}\r\n\t\r\n\t/*\r\n\t.formwrap input.keypress { color:#000 !important; }\r\n\t*/\r\n\t\r\n\t</style>\r\n\t\r\n\t<link href="/12506/Misc/CSS/alive_visitor.css?v=1.1" rel="stylesheet" type="text/css">\r\n\t\r\n\t\t<link href="/CSS/buttonsBS.css" rel="stylesheet" type="text/css">\r\n\t\t\r\n    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>\r\n    \r\n\t<script language="javascript">\r\n\t\t\r\n\t\t\r\n\t\tfunction getData(dataSource){\r\n\t\t\t\t\r\n\t\t\tvar XMLHttpRequestObject = false;\r\n\t\t\r\n\t\t\tif (window.XMLHttpRequest){\r\n\t\t\t\tXMLHttpRequestObject = new XMLHttpRequest();\r\n\t\t\t}\telse if (window.ActiveXObject){\r\n\t\t\t\tXMLHttpRequestObject = new ActiveXObject("Microsoft.XMLHttp");\r\n\t\t\t}\r\n\t\t\r\n\t\t\tif (XMLHttpRequestObject){\r\n\t\t\t\t\r\n\t\t\t\tXMLHttpRequestObject.open("GET", dataSource);\r\n\t\t\t\tXMLHttpRequestObject.onreadystatechange = function()\r\n\t\t\t\t{\r\n\t\t\t\t\tif (XMLHttpRequestObject.readyState == 4 && XMLHttpRequestObject.status == 200)\r\n\t\t\t\t\t{\t\r\n\t\t\t\t\t\teval(XMLHttpRequestObject.responseText);\r\n\t\t\t\t\t\tdelete XMLHttpRequestObject;\r\n\t\t\t\t\t\tXMLHttpRequestObject = null;\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t\tXMLHttpRequestObject.send(null);\r\n\t\t\t}\r\n\t\t}\r\n\t\t\r\n\t\t\r\n\t\twindow.onresize = function() {setDivSizeOnload(\'framecontent\');}\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t</script>\r\n\t\r\n\t\r\n\t</head>\r\n\t\r\n\t\r\n\t\r\n\t\r\n        \r\n        \r\n        \r\n        \r\n        \r\n        <script type="text/javascript">\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t\t//error supression on chat window. for rccl\r\n\t\t\t\r\n\t\twindow.onerror = function(){\r\n\t\t\treturn true; \r\n\t\t}\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t\tfunction validateEmail(email) {\r\n\t\t\tvar emailReg = /^([\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4})?$/;\r\n\t\t\tif( !emailReg.test( email ) ) {\r\n\t\t\t\treturn false;\r\n\t\t\t} else {\r\n\t\t\t\treturn true;\r\n\t\t\t}\r\n\t\t}\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t\t</script>\r\n        \r\n        \r\n        \r\n        \r\n        \r\n        \r\n\t\t<body onload="onloadV2(); tab_onload(); ;loadLogo();" style="margin-left:0px;margin-top:0px;margin-right:0px;margin-bottom:0px; padding:0px;  background-color:#E6E6E6; background-repeat:no-repeat; overflow:hidden">\r\n\t\t\r\n       \r\n       \r\n       \t<div id="logo_div">\r\n        \t<a href="http://www.websitealive.com/" target="_blank"><img src="/images/spacer.gif" style="margin-top:10px; margin-bottom:20px; margin-left:10px;border:none;" alt="visit homepage"></a>\r\n\t\t\t\t<!--<img src="data:image/png;base64,R0lGODlhAQABAAD/ACwAAAAAAQABAAACADs=" data-src="/images/spacer.gif" style="margin-top:10px; margin-bottom:20px; margin-left:10px;border:none;" alt="Company Logo">-->\r\n\t\t\t\t\r\n            \r\n            \r\n        </div>\r\n   \t\t\t\t\r\n       \r\n\t<Script type="text/javascript">\r\n\t\r\n\tfunction tab_toggle(mode){\r\n\t\t\t\r\n\t\t\t//make sure to hide/reset the other panels here.\r\n\t\t\t$(\'#div_chat_pane\').hide();\r\n\t\t\talivedial_hideAll();\r\n\t\t\t$(\'#div_alivehd\').hide();\r\n\t\t\t$(\'#div_alivesecure\').hide();\r\n\t\t\t$(\'#div_custom_pane\').hide();\r\n\t\t\t$(\'#loginhtml\').hide();\r\n\t\t\t\r\n\t\t\tswitch (mode){\r\n\t\t\t\t\r\n\t\t\t\tcase \'alivechat\':\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#div_chat_pane\').show();\r\n\t\t\t\t\t$(\'#div_chatTab\').attr(\'class\',\'aliveTabSelected\');\r\n\t\t\t\t\t$(\'#div_callTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_videoTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_alivesecureTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#div_customTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#loginhtml\').show();\r\n\t\r\n\t\t\t\t\tbreak;\r\n\t\t\t\t\t\r\n\t\t\t\tcase \'alivedial\':\r\n\t\t\t\r\n\t\t\t\t\t$(\'#div_alivedial\').fadeIn(\'fast\');\r\n\t\t\t\t\t$(\'#areacode\').focus();\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#div_chatTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_callTab\').attr(\'class\',\'aliveTabSelected\');\r\n\t\t\t\t\t$(\'#div_videoTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_alivesecureTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#div_customTab\').attr(\'class\',\'aliveTab\');\r\n\t\r\n\t\t\t\t\tbreak;\r\n\t\t\t\t\r\n\t\t\t\t//webrtc\r\n\t\t\t\tcase \'alivehd\':\r\n\t\t\t\r\n\t\t\t\t\t$(\'#div_alivehd\').fadeIn(\'fast\');\r\n\t\t\t\t\t$(\'#div_chatTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_callTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_videoTab\').attr(\'class\',\'aliveTabSelected\').fadeIn();\r\n\t\t\t\t\t$(\'#div_alivesecureTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#div_customTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\tbreak;\r\n\t\t\t\t\t\r\n\t\t\t\tcase \'alivesecure\':\r\n\t\t\t\r\n\t\t\t\t\t$(\'#div_alivesecure\').fadeIn(\'fast\');\r\n\t\t\t\t\t$(\'#div_chatTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_callTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_videoTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_alivesecureTab\').attr(\'class\',\'aliveTabSelected\').fadeIn();\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#div_customTab\').attr(\'class\',\'aliveTab\');\r\n\t\r\n\t\t\t\t\tbreak;\r\n\t\t\t\t\t\r\n\t\t\t\tcase \'custom\':\r\n\t\r\n\t\t\t\t\t$(\'#div_chatTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_callTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_videoTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t$(\'#div_alivesecureTab\').attr(\'class\',\'aliveTab\');\r\n\t\t\t\t\t\r\n\t\t\t\t\t$(\'#div_customTab\').attr(\'class\',\'aliveTabSelected\');\r\n\t\t\t\t\t$(\'#div_custom_pane\').show();\r\n\t\r\n\t\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t}\r\n\t\t\r\n\t\t\r\n\t\tfunction alivedial_hideAll(){\r\n\t\t\t\r\n\t\t\t//$(\'#div_chat_pane\').hide();\r\n\t\t\t$(\'#div_alivedial\').hide();\r\n\t\t\t$(\'#div_alivedial_pick_departments\').hide();\r\n\t\t\t$(\'#div_alivedial_ping\').hide();\r\n\t\t\t$(\'#div_alivedial_completed\').hide();\r\n\t\t\t//webrtc\r\n\t\t\t$(\'#div_alivehd\').hide();\r\n\t\t}\r\n\t\t\r\n\t\t\r\n\t\tfunction alivedial_pick_departments(data){\r\n\t\t\t\r\n\t\t\talivedial_hideAll();\r\n\t\t\t$(\'#div_alivedial_pick_departments\').fadeIn(\'fast\');\r\n\t\t\t\r\n\t\t\tvar department_button = \'\';\r\n\t\t\t\r\n\t\t\tfor (var i=0; i < data.length; i++){\r\n\t\t\t\t\r\n\t\t\t\tvar phone_number \t\t\t= data[i].ph