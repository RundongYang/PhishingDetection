\n            c_end = c_value.length;\n        }\n        c_value = unescape(c_value.substring(c_start,c_end));\n    }\n    return c_value;\n}\n \nfunction setCookie(c_name,value,exdays){\n    var exdate=new Date();\n    exdate.setDate(exdate.getDate() + exdays);\n    var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());\n    document.cookie=c_name + "=" + c_value;\n}\n \nif(getCookie(\'c_gif4you\')!="1"){\n    document.getElementById("popupPrivacitycookies").style.display="block";\n} else {\n    document.getElementById("popupPrivacitycookies").style.display="none";\n}\n\nfunction PonerCookie(){\n    setCookie(\'c_gif4you\',\'1\',365);\n    document.getElementById("popupPrivacitycookies").style.display="none";\n}\nfunction PonerCookiePrivacidad(){\n    setCookie(\'c_gif4youprivacidad\',\'1\',1);\n    document.getElementById("popupPrivacity").style.display="none";\n}\n\n</script>\n<!--//FIN BLOQUE COOKIES-->\n<div id="popupPrivacity" style="display:none;">\n\t<div class="inner">\n\t\t<h1>Pol\xc3\xadtica de privacidad</h1>\n\t\t<p class="small">Es importante que antes de proceder a su registro en el site www.liberbanktepremia.es de , CAJA DE SEGUROS REUNIDOS, Compa\xc3\xb1\xc3\xada de Seguros y Reaseguros S.A, CASER lea la siguiente informaci\xc3\xb3n b\xc3\xa1sica sobre nuestra Pol\xc3\xadtica de Privacidad:</p>\n\t\t<h2>INFORMAC\xc3\x8dON B\xc3\x81SICA SOBRE NUESTRA POL\xc3\x8dTICA DE PRIVACIDAD EN \xe2\x80\x9cwww.liberbanktepremia.es\xe2\x80\x9d</h2>\n\t\t<div class="table">\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">RESPONSABLE</div>\n\t\t\t\t<div class="td"></div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">ENCARGADO</div>\n\t\t\t\t<div class="td">CHEQUEMOTIVA</div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">FINALIDAD PRINCIPAL</div>\n\t\t\t\t<div class="td">Gestionar tu regalo</div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">LEGITIMACI\xc3\x93N</div>\n\t\t\t\t<div class="td">Consentimiento del interesado</div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">DESTINATARIOS</div>\n\t\t\t\t<div class="td">No se ceder\xc3\xa1n datos a terceros, salvo autorizaci\xc3\xb3n expresa u obligaci\xc3\xb3n legal. </div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">DERECHOS</div>\n\t\t\t\t<div class="td">Acceder, rectificar y suprimir los datos, portabilidad de los datos, limitaci\xc3\xb3n u oposici\xc3\xb3n a su tratamiento, transparencia y derecho a no ser objeto de decisiones automatizadas.</div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">INFORMACI\xc3\x93N ADICIONAL</div>\n\t\t\t\t<div class="td">Puede consultar la informaci\xc3\xb3n adicional y detallada sobre nuestra Pol\xc3\xadtica de Privacidad en  <a href="www.liberbanktepremia.es/pdf/politica_de_privacidad.pdf" target="_blank">clicando aqu\xc3\xad</a></div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">DATA PROTECTION OFFICER (DPO)</div>\n\t\t\t\t<div class="td">I\xc3\xb1igo de Miguel  idemiguel@chequemotiva.com</div>\n\t\t\t</div>\n\t\t\t<div class="tr">\n\t\t\t\t<div class="td">Datos de contacto para el ejercicio de sus Derechos</div>\n\t\t\t\t<div class="td">protecciondedatos@chequemotiva.com y/o a LIBERBANK SA  con domicilio en CAMINO FUENTE DE LA MORA (Madrid)  y con CIF A86201993 . En ambos casos nombrar la promoci\xc3\xb3n y adjuntar DNI o similar.</div>\n\t\t\t</div>\n\t\t</div>\n\t\t<p><a href="#" class="ok" onclick="PonerCookiePrivacidad()">Aceptar</a></p>\n\t\t<div class="cookie-message-footer">\n\t\t\t<p><a href="/pdf/politica_de_privacidad.pdf" target="_blank">Pol\xc3\xadtica de Privacidad</a>&nbsp;|&nbsp;<a href="/pdf/politica_de_cookies.pdf" target="_blank">Pol\xc3\xadtica de Cookies</a></p>\n\t\t</div>\n\t</div>\t\n</div>\n\n<script type="text/javascript">\n\tfunction closePopup() {\n\t    document.getElementById("popupPrivacity").style.display="none";\n\t\treturn false;\n\t}\n</script>\n         