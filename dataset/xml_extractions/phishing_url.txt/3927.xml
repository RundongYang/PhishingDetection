b'<html>\n  <head>\n    <title>ATT Login Redirect</title>\n    <script type="text/javascript">\n    // Check to see what hostname is used to login to appliance - use the right config based on it\n    var hname = window.location.hostname.toLowerCase();\n    var dcsuffix = \'\';\n    if (hname.indexOf("aldc") == 0 || hname.indexOf("ffdc") == 0)\n    {\n        dcsuffix = "DC";\n    }\n    else if (hname.indexOf("wwwagt") == 0 || hname.indexOf("wwwfft") == 0)\n    {\n        dcsuffix = "TMP";\n    }\n    var purl = window.location.href;\n    var turl = encodeURIComponent(purl);\n    if ("" == "2")\n    {\n\t    window.location = "/isam/sps/oidc/rp/ATT-RP"+dcsuffix+"/kickoff/ATT-PwdPlus"+dcsuffix+"?Target="+turl;\n    }\n    else if ("" == "3")\n    {\n\t    window.location = "/isam/sps/oidc/rp/ATT-RP"+dcsuffix+"/kickoff/ATT-Token"+dcsuffix+"?Target="+turl;\n    }\n    else if ("" == "4")\n    {\n\t    window.location = "/isam/sps/oidc/rp/ATT-RP"+dcsuffix+"/kickoff/ATT-Opus"+dcsuffix+"?Target="+turl;\n    }\n    else\n    {\n\t    window.location = "/isam/sps/oidc/rp/ATT-RP"+dcsuffix+"/kickoff/ATT-Password"+dcsuffix+"?Target="+turl;\n    }\n\n    </script>\n  </head>\n  <body>\n    <form></form>\n  </body>\n</html>\n'