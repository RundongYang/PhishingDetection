             }\r\n\r\n                    $(\'#imgUserLogo\').attr("src", valueParts[0] + $(\'#imgUserLogo\').attr("src"));\r\n                }   \r\n            }\r\n            $(\'#imgEWSLogo\').css("visibility", "visible");\r\n            $(\'#imgUserLogo\').css("visibility", "visible");\r\n\r\n            $(\'#btnPunch\').click(ActionOnPunch);\r\n\r\n            //NTI-150917-IT6A - KL - 09/17/2015\r\n            $("#btnLogin,#btnPunchOut,#btnTransfer,#btndailysubmit").click(function () {\r\n                var clickReturn = validateInputFields();\r\n\r\n                if ($(\'#txtPunchMsg\').val().indexOf(\'password\') >= 0) {\r\n                    if ($(\'#txtUserName\').val().length == 0 || $(\'#txtPassword\').val().length == 0) {\r\n                        $(\'#txtPassword\').attr(\'aria-label\', $(\'#txtPunchMsg\').val());\r\n                    }\r\n                }\r\n                else {\r\n                    if ($(\'#txtUserName\').val().length == 0 || $(\'#txtPassword\').val().length == 0) {\r\n                        $(\'#txtUserName\').attr(\'aria-label\', $(\'#txtPunchMsg\').val());\r\n                    }\r\n                }\r\n\r\n                return clickReturn;\r\n            });\r\n            //EOC\r\n\r\n            //DS: Set the background image\r\n            if (typeof(imgUrl) === \'undefined\') {\r\n                imgUrl = "https://source.unsplash.com/1024x768?nature,mountains,forest,water"; \r\n            }\r\n            $(\'.bg-img\').css(\'background-image\', \'url("\' + imgUrl + \'")\');\r\n\r\n            $(\'#lblErr\').attr("aria-live", "assertive");\r\n            $(".login-wrap").css({ "height": $(window).height() });\r\n            $(".login-wrap").css({ "overflow-y": "overlay" });//NTI-190111-RD15A - KL - 01/11/2019 -  add overflow-y:overlay and padding\r\n            $(".login-form-wrap").css({ "padding": "30px 10% 0px" });\r\n            setTimeout(function () {\r\n                $(".novaerrmsg").hide(2000);\r\n            }, 10000);\r\n\r\n            setTimeout(function () {\r\n                $(\'.form-field #txtUserName\').focus(); //NEU-160512-PMO1A AP set focus on ID inputfield\r\n            });\r\n            \r\n\r\n            $(\'#ClientIDRegex\').css(\'display\', \'none\');\r\n            $(\'#UserNameRegex\').css(\'display\', \'none\');\r\n            $(\'#PasswordRegex\').css(\'display\', \'none\');\r\n\r\n            $(\'#txtClientID\').focusout(function () {\r\n                if ($(\'#ClientIDRegex\').prop("style")["visibility"] == \'visible\') {\r\n                    $(\'#ClientIDRegex\').css(\'display\', \'\');\r\n                }\r\n            });\r\n\r\n            $(\'#txtUserName\').focusout(function () {\r\n                $(\'#txtUserName\').removeAttr(\'aria-label\');\r\n\r\n                if ($(\'#UserNameRegex\').prop("style")["visibility"] == \'visible\') {\r\n                    $(\'#UserNameRegex\').css(\'display\', \'\');\r\n                }\r\n            });\r\n\r\n            $(\'#txtPassword\').focusout(function () {\r\n                $(\'#txtPassword\').removeAttr(\'aria-label\');\r\n\r\n                if ($(\'#PasswordRegex\').prop("style")["visibility"] == \'visible\') {\r\n                    $(\'#PasswordRegex\').css(\'display\', \'\');\r\n                }\r\n            });\r\n\r\n\r\n\r\n            //IsMobileEnabled(); //NTI-150130-RD8A - GZ - 02/12/2015\r\n\r\n            $("#txtUserName").keypress(function (event) {\r\n                if (event.which == 13) {\r\n                    $(\'#txtPassword\').focus();\r\n                } \r\n            });\r\n\r\n            $("#txtPassword").keypress(function (event) {\r\n                if (event.which == 13) {\r\n                    //DS: AVX-120511-DS10A: 22nd June 2012: Default button handling\r\n                    if (typeof defaultLoginButton != \'undefined\') {\r\n                        if (defaultLoginButton == \'E\') {\r\n                            $(\'#btnLogin\').focus();\r\n                            $(\'#btnLogin\').click();\r\n                            return false;\r\n                        }\r\n                        else if (defaultLoginButton == \'T\') {\r\n                            $(\'#btnTransfer\').focus();\r\n                            $(\'#btnTransfer\').click();\r\n                            return false;\r\n                        }\r\n                    }\r\n                    //By default it is punch button (defaultLoginButton == \'P\')\r\n                    $(\'#btnPunch\').focus();\r\n                    $(\'#btnPunch\').click();\r\n                    return false;\r\n                }\r\n            });\r\n\r\n            //NTI-170922-RD39A - ARG - 09/27/2017\r\n            $("#txtImageAuth").keypress(function (event) {\r\n                if (event.which == 13) {\r\n                    $(\'#btnImageAuth\').click();\r\n                }\r\n            });\r\n\r\n            //NTI-170608-RD15A - KL - 06/09/2017 - no longer needed\r\n            //// PTS - 120604 - DS10B - set the parent height when this is called as a frame within ewsloginII.aspx\r\n            //if (parent.adjustFrameWidthHeight != null) {\r\n            //    parent.adjustFrameWidthHeight($(document).width(), $(document).height());\r\n            //}\r\n\r\n            //NTI-121022-JJA-KL-11/16/2012 -geo location tracking\r\n            if (!(typeof enableGeoLocation === "undefined") && enableGeoLocation == \'true\') {\r\n                getEWSGeoLocation();\r\n            }\r\n            var objtxtPassword = getObj("changePWsecq1_txtPassword");\r\n            if (objtxtPassword) $(\'#changePWsecq1_txtPassword\').focus();\r\n\r\n        });\r\n\r\n        //NTI-121022-JJA-KL-11/16/2012       \r\n        function getEWSGeoLocation() {\r\n            if (navigator.geolocation) {\r\n                navigator.geolocation.getCurrentPosition(storePosition);\r\n            }\r\n            // else { document.getElementById("hiddenGPSCoords").value = "notsupported"; } //"Geolocation is not supported by this browser."\r\n        }\r\n\r\n        function storePosition(position) {\r\n            document.getElementById("hiddenGPSCoords").value = position.coords.latitude + "," + position.coords.longitude;  //+ "," + position.coords.accuracy; \r\n        }\r\n        //EOC\r\n\r\n    //NTI-151029-QA6A -AP - 12/23/2015  \r\n    var currentLanguage = \'en\';\r\n        if (!(typeof jsLanguage === "undefined") && jsLanguage != "") {\r\n            currentLanguage = jsLanguage;\r\n        }\r\n        var option = {\r\n            lng: currentLanguage,\r\n            ns: {\r\n                namespaces: [\'login\', \'jsModalPopup\', \'jsnn6\', \'jsShared\', \'jsUcGroupList\', \'jsUcXferGroupsNewUI\'],\r\n                defaultNs: \'login\'\r\n            },\r\n            fallbackLng: \'en\',\r\n            getAsync: false\r\n        };\r\n        i18n.init(option);\r\n\r\n        $(window).resize(function () {\r\n            $(".login-wrap").css({ "height": $(window).height() });\r\n        });\r\n\r\n        function focusState(element) {\r\n            element.style.backgroundColor = "#FFFFDE";\r\n            element.style.borderColor = "#000000";\r\n            element.style.color = "#000000";\r\n        }\r\n        function blurState(element) {\r\n            element.style.backgroundColor = "#FFFFFF";\r\n            element.style.borderColor = "#777777";\r\n            element.style.color = "#777777";\r\n        }\r\n\r\n        function IsMobileEnabled() {\r\n            //make a Ajax call to get case sensitivity...\r\n            $.ajax({\r\n                type: "POST",\r\n                url: "ewslogin.aspx/IsMobileEnabled",\r\n                contentType: "application/json; charset=utf-8",\r\n                data: "{}",\r\n                dataType: "json",\r\n                timeout: 50000,\r\n                success: function (msg) {\r\n                    if (msg.d == true) {\r\n\r\n                        var hostName = window.location.hostname;\r\n                        var pathArray = window.location.pathname.split("/");\r\n                        var serverName = "/" + pathArray[1];\r\n                        var queryString = window.location.search;\r\n                        var additionalQuery = "&Source=ews";\r\n\r\n                        //$(\'#mobilesitelink\').show();\r\n                        $(\'#mobilesitelink\').attr(\'href\', serverName + \'/mobile/ewslogin.aspx\' + queryString + additionalQuery);\r\n\r\n                        SA.redirection_mobile({\r\n                            mobile_url: hostName + serverName + "/mobile/ewslogin.aspx",\r\n                            keep_query: "true",\r\n                            additional_query: additionalQuery // added new parameter to add to query string\r\n                        });\r\n                    }\r\n                    else {\r\n                        $(\'#mobilesitelink\').hide();\r\n                    }\r\n                },\r\n                error: function (req, status, ex) {\r\n                    if (navigator.appName != \'Microsoft Internet Explorer\') {\r\n                        console.log("Error: " + ex);\r\n                        $(\'#mobilesitelink\').hide();\r\n                    }\r\n                }\r\n            });\r\n        }\r\n        </script>\r\n\r\n        \r\n</head>\r\n<body onkeypress="disabledPostBack()">\r\n    <div id="UsedNOVA4000ThemePath" style="display:none;" data-info=""></div>\r\n    <div id="bgImg" class="bg-img col-md-12">\r\n        <div class="nova-widgetcont">\r\n            \r\n            <div class="weather-wrap">\r\n                <div>\r\n                    <table id="PubMsg_pubmsgtable" style="width:100%;border-collapse: collapse;border:0;" cellpadding="0">\r\n\t<tr style="border-color:white;">\r\n\t\t<td style="text-align:center;"></td>\r\n\t</tr>\r\n</table>\r\n\r\n<div id="PubMsg_plhPubMsg2">\r\n\r\n</div>\r