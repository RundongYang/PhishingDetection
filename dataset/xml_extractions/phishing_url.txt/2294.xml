n.current && angularMyATTSession.current.selectedAccountInfo && angularMyATTSession.current.selectedAccountInfo.AccountNumber;\n        \t\t};\n        \t\t\n        \t\t// get the account type of the currently logged in account user.\n        \t\tvar getSelectedAccountType = function() {\n        \t\t\treturn angularMyATTSession && angularMyATTSession.current && angularMyATTSession.current.selectedAccountInfo && angularMyATTSession.current.selectedAccountInfo.accountType;\n        \t\t};\n        \t\t\n        \t\t// get the authenticated id of the logged in user\n        \t\tvar getAuthenticatedId = function() {\t\n        \t\t\treturn angularMyATTSession && angularMyATTSession.current && angularMyATTSession.current.AuthenticatedID && angularMyATTSession.current.AuthenticatedID.split("@")[0];\n        \t\t}\n        \t\t\n        \t\t// get all accounts mapped to the current user. Ignore the accounts with combined bill for wireline and uverse set to true.\n        \t\t// return object with all valid wireless, wireline, and uverse accounts\n        \t\tvar getAllNonCombinedAccountNumbers = function() {\n        \t\t\tif (angularMyATTSession && angularMyATTSession.current) {\n        \t\t\t\t// check for wireless accounts if they are combined.\n        \t\t\t\tvar wirelessAccounts = angularMyATTSession.current.wirelessAccounts;\n        \t\t\t\tvar wirelessAccountDetails = angularMyATTSession.current.accountDetailResponseObj.WirelessAccountDetails;\n        \t\t\t\t// filter out accounts having either uverse combined bill or wireline combined bill.\n        \t\t\t\tvar filteredAccountNumbers = wirelessAccounts.filter(function(wirelessAccount) {\n        \t\t\t\t\t// wirelessAccountDetails can be an array containing multiple account details or single obj. Handle array elements differently\n        \t\t\t\t\tif (wirelessAccountDetails instanceof Array) {\n        \t\t\t\t\t\treturn wirelessAccountDetails.filter(function(accountDetail) {\n        \t\t\t\t\t\t\treturn accountDetail.AccountNumber === wirelessAccount && accountDetail.WirelineCombinedBilling === "false" && accountDetail.UverseCombinedBilling === "false";\n        \t\t\t\t\t\t}).length > 0;\n        \t\t\t\t\t} else {\n        \t\t\t\t\t\tif(wirelessAccountDetails.AccountNumber === wirelessAccount && wirelessAccountDetails.WirelineCombinedBilling === "false" && wirelessAccountDetails.UverseCombinedBilling === "false") {\n        \t\t\t\t\t\t\treturn true;\n        \t\t\t\t\t\t} else {\n        \t\t\t\t\t\t\treturn false;\n        \t\t\t\t\t\t}\n        \t\t\t\t\t}\n        \t\t\t\t});\n        \t\t\t\t\n        \t\t\t\treturn { \n        \t\t\t\t\t"WirelessAccountData": filteredAccountNumbers, \n        \t\t\t\t\t"WirelineAccountData": angularMyATTSession.current.wirelineAccounts, \n        \t\t\t\t\t"UverseAccountData": angularMyATTSession.current.uverseAccounts \n        \t\t\t\t};\n        \t\t\t} else {\n        \t\t\t\tconsole.log("AT: Unable to get the Session details");\n        \t\t\t\treturn;\n        \t\t\t}\n        \t\t};\n        \t\t\n        \t\t// get Autopay status of the account for a specific account type. Example, WirelessAccountData, UverseAccountData\n        \t\tvar getAutopayStatus = function(accountNumber, accountType) {\n        \t\t\t// wrapping jQuery.ajax within Promise.resolve to convert jqXHR object returned into a real promise\n        \t\t\treturn Promise.resolve(jQuery.ajax({\n        \t\t\t\ttype: "POST",\n        \t\t\t\turl: \'/best/resources/auth/payment/paymentplan/get\',\n        \t\t\t\tcontentType: "application/json; charset=utf-8",\n        \t\t\t\theaders: {\n        \t\t\t\t\t\'X-Requested-By\': \'target\'\n        \t\t\t\t},\n        \t\t\t\tdata: JSON.stringify({\n        \t\t\t\t\tCommonData: {\n        \t\t\t\t\t\tAppName: "MYATT",\n        \t\t\t\t\t\t[accountType]: [{ AccountNumber: accountNumber }]\n        \t\t\t\t\t}\n        \t\t\t\t})\n        \t\t\t}));\n        \t\t}\n        \t\t\n        \t\t// get account balance for a specific account type to determine if there is an overdue balance.\n        \t\tvar getAccountBalance = function(accountNumber, accountType) {\n        \t\t\t// wrapping jQuery.ajax within Promise.resolve to convert jqXHR object returned into a real promise\n        \t\t\treturn Promise.resolve(jQuery.ajax({\n        \t\t\t\ttype: "POST",\n        \t\t\t\turl: \'/best/resources/auth/payment/general/getAccountBalance\',\n        \t\t\t\tcontentType: "application/json; charset=utf-8",\n        \t\t\t\theaders: {\n        \t\t\t\t\t\'X-Requested-By\': \'target\'\n        \t\t\t\t},\n        \t\t\t\tdata: JSON.stringify({\n        \t\t\t\t\tCommonData: {\n        \t\t\t\t\t\tAppName: "MYATT",\n        \t\t\t\t\t\t[accountType]: [{ AccountNumber: accountNumber }]\n        \t\t\t\t\t}\n        \t\t\t\t})\n        \t\t\t}));\n        \t\t}\n        \n        \t\t// get the account balance of the wireless account currently logged in. \n        \t\tvar currentUserPastDueWithoutPA = function() {\n        \t\t\tvar selectedAccountNumber = getSelectedAccountNumber();\n        \t\t\t\n        \t\t\tif (selectedAccountNumber) {\n        \t\t\t\treturn getAccountBalance(selectedAccountNumber, "WirelessAccountData").then(function(data) {\n        \t\t\t\t\tif (data.Result && data.Result.Status && (/SUCCESS/i).test(data.Result.Status)) {\n        \t\t\t\t\t\treturn data && data.PastDue && parseInt(data.PastDue) > 0 && data.AdditionalInformation && data.AdditionalInformation.PaymentArrangementExist === "false";\n        \t\t\t\t\t} else {\n        \t\t\t\t\t\treturn Promise.reject("Reason: getAccountBalance API failed"); \n        \t\t\t\t\t}\n        \t\t\t\t});\n        \t\t\t} else {\n        \t\t\t\treturn Promise.reject("Reason: Unable to get selectedAccountNumber");\n        \t\t\t}\n        \t\t}\n        \t\t\n        \t\tvar launchTheMboxP136_at = function(mboxName, userStory, usecase) {\n        \t\t\tvar params = {\n        \t\t\t\t"child_campaign": userStory,\n        \t\t\t\t"launchedFrom": userStory,\n        \t\t\t\t"user.usecase": usecase\n        \t\t\t};\n        \t\t\tadobe.target.getOffer({\n        \t\t\t\t"mbox": mboxName,\n        \t\t\t\t"params": params,\n        \t\t\t\t"success": function(offers) {\n        \t\t\t\t\tadobe.target.applyOffer({\n        \t\t\t\t\t\t"mbox": mboxName,\n        \t\t\t\t\t\t"params": params,\n        \t\t\t\t\t\t"offer": offers\n        \t\t\t\t\t});\n        \t\t\t\t},\n        \t\t\t\t"error": function(status, error) {\n        \t\t\t\t\tif (console && console.log) {\n        \t\t\t\t\t\tconsole.log(status);\n        \t\t\t\t\t\tconsole.log(error);\n        \t\t\t\t\t}\n        \t\t\t\t},\n        \t\t\t\t"timeout": 5000\n        \t\t\t});\n        \t\t}\n        \n        \t\tvar p136ChildCampaignIsLaunched = false;\n        \t\tvar USP136_at_mainFunction = function() {\n        \t\t\tvar count_P136_at = 0;\n        \n        \t\t\tvar waiter_P136_at = setInterval(function() {\n        \t\t\t\tif (window.location.hash.indexOf("#/Payments") > -1 || window.location.hash.indexOf("#/payAllConfirmation") > -1 || window.location.hash.indexOf("#/makePaymentConfirmation") > -1) {\n        \t\t\t\t\tclearInterval(waiter_P136_at);\n        \t\t\t\t\t// Delay the execution of the code to handle for Application API completion, Example: getAggregatedPaymentData API\n        \t\t\t\t\tsetTimeout(function() {\n        \t\t\t\t\t\t// for payAll confirmation, check for multiple services\n        \t\t\t\t\t\tif (window.location.hash.indexOf("#/payAllConfirmation") > -1) {\n        \t\t\t\t\t\t\tvar nonCombinedAccountNumbers = getAllNonCombinedAccountNumbers();\n        \t\t\t\t\t\t\tif (nonCombinedAccountNumbers) {\n        \t\t\t\t\t\t\t\tvar promises = [];\n        \t\t\t\t\t\t\t\tvar promiseAccounts = []; // Variable to keep track of each promise response with the account number.\n        \t\t\t\t\t\t\t\t\n        \t\t\t\t\t\t\t\t// prioritize on wireless, uverse, and wireline.\n        \t\t\t\t\t\t\t\tif (nonCombinedAccountNumbers[\'WirelessAccountData\'].length > 0) {\n        \t\t\t\t\t\t\t\t\tnonCombinedAccountNumbers[\'WirelessAccountData\'].forEach(function(accountNumber) {\n        \t\t\t\t\t\t\t\t\t\tpromiseAccounts.push(accountNumber);\n        \t\t\t\t\t\t\t\t\t\tpromises.push(getAutopayStatus(accountNumber, \'WirelessAccountData\'));\n        \t\t\t\t\t\t\t\t\t});\n        \t\t\t\t\t\t\t\t}\n        \t\t\t\t\t\t\t\tif (nonCombinedAccountNumbers[\'UverseAccountData\'].length > 0) {\n        \t\t\t\t\t\t\t\t\tnonCombinedAccountNumbers[\'UverseAccountData\'].forEach(function(accountNumber) {\n        \t\t\t\t\t\t\t\t\t\tpromiseAccounts.push(accountNumber);\n        \t\t\t\t\t\t\t\t\t\tpromises.push(getAutopayStatus(accountNumber, \'UverseAccountData\'));\n        \t\t\t\t\t\t\t\t\t});\n        \t\t\t\t\t\t\t\t}\n        \t\t\t\t\t\t\t\tif (nonCombinedAccountNumbers[\'WirelineAccountData\'].length > 0) {\n        \t\t\t\t\t\t\t\t\tnonCombinedAccountNumbers[\'WirelineAccountData\'].forEach(function(accountNumber) {\n        \t\t\t\t\t\t\t\t\t\tpromiseAccounts.push(accountNumber);\n        \t\t\t\t\t\t\t\t\t\tpromises.push(getAutopayStatus(accountNumber, \'WirelineAccountData\'));\n        \t\t\t\t\t\t\t\t\t});\n        \t\t\t\t\t\t\t\t}\n        \n        \t\t\t\t\t\t\t\t// once all the promises are resolved, check if any of the accounts didn\'t have autopay, and then pass that information to the child.\n        \t\t\t\t\t\t\t\tPromise.all(promises).then(function(response) {\n        \t\t\t\t\t\t\t\t\tfor (var i=0; i<response.length; ++i) {\n        \t\t\t\t\t\t\t\t\t\tif (response[i].Result && response[i].Result.Description != null && (/Payment plan not found/i).test(response[i].Result.Description)) {\n        \t\t\t\t\t\t\t\t\t\t\t// focusAccountNumber should be passed to the child campaign in order to automatically allow selection of account on the enroll page.\n        \t\t\t\t\t\t\t\t\t\t\tif (!p136ChildCampaignIsLaunched) {\n        \t\t\t\t\t\t\t\t\t\t\t\tsessionStorage.setItem("focusAccountNumber", promiseAccounts[i]);\n        \t\t\t\t\t\t\t\t\t\t\t\tconsole.log("AT: P-136 Launching child campaign for AutopayOnPayAllConfirmation");\n        \t\t\t\t\t\t\t\t\t\t\t\tlaunchTheMboxP136_at(\'P136_Child\', \'P136\', "AutopayOnPayAllConfirmation");\n        \t\t\t\t\t\t\t\t\t\t\t\tp136ChildCampaignIsLaunched = true;\n        \t\t\t\t\t\t\t\t\t\t\t\tbreak;\n        \t\t\t\t\t\t\t\t\t\t\t}\n        \t\t\t\t\t\t\t\t\t\t}\n        \t\t\t\t\t\t\t\t\t}\n        \t\t\t\t\t\t\t\t\tif (!p136ChildCampaignIsLaunched) {\n        \t\t\t\t\t\t\t\t\t\taccountChangeEventSetup();\n        \t\t\t\t\t\t\t\t\t}\n        \t\t\t\t\t\t\t\t}).catch(function(error) {\n        \t\t\t\t\t\t\t\t\tconsole.log("AT: Error getting autopay status : " + error);\n        \t\t\t\t\t\t\t\t});\n        \t\t\t\t\t\t\t}\n        \t\t\t\t\t\t} else {\t\n        \t\t\t\t\t\t\t// we are on the payments page or makePaymentConfirmation page\n        \t\t\t\t\t\t\tvar accountType = getSelectedAccountType();\n        \t\t\t\t\t\t\tvar accountTypeKey = "WirelessAccountData";\n        \t\t\t\t\t\t\tif (accountType.toUpperCase() === "WIRELINE") {\n        \t\t\t\t\t\t\t\taccountTypeKey = "WirelineAccountData";\n        \t\t\t\t\t\t\t} else if (accountType.toUpperCase() === "UVERSE") {\n        \t\t\t\t\t\t\t\taccountTypeKey = "UverseAccountData";\n        \t\t\t\t\t\t\t}\n        \t\t\t\t\t\t\tvar selectedAccountNumber = getSelectedAccountNumber();\n        \t\t\t\t\t\t\tvar useCase = (window.location.hash.indexOf("#/Payments") > -1)? "AutopayOnPaymentActivity" : "AutopayOnPaymentConfirmation";\n        \t\t\t\t\t\t\tvar useCaseStorageKey = (window.location.hash.indexOf("#/Payments") > -1)? "autopayonpaymentactivity" : "autopayonpaymentconfirmation";\n        \t\t\t\t\t\t\t\n        \t\t\t\t\t\t\t// if the current user is wireless and past due without a payment arrangement, ask user to make payment arrangements\n        \t\t\t\t\t\t\tif (accountType.toU