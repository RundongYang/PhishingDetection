_DE"){\r\n\t\t\twin = window.open(\'https://3dsec.postfinance.ch/acspage/de_DE_base_postfinance/info_cvv.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "en_US"){\r\n\t\t\twin = window.open(\'en_US_base_postfinance/info_cvv.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "fr_FR"){\r\n\t\t\twin = window.open(\'fr_FR_base_postfinance/info_cvv.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "it_IT"){\r\n\t\t\twin = window.open(\'it_IT_base_postfinance/info_cvv.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}\r\n\t}\r\n\telse if (text == "chname"){\r\n\t\tif (optinLang == "de_DE"){\r\n\t\t\twin = window.open(\'https://3dsec.postfinance.ch/acspage/de_DE_base_postfinance/info_chName.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "en_US"){\r\n\t\t\twin = window.open(\'en_US_base_postfinance/info_chName.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "fr_FR"){\r\n\t\t\twin = window.open(\'fr_FR_base_postfinance/info_chName.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "it_IT"){\r\n\t\t\twin = window.open(\'it_IT_base_postfinance/info_chName.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}\r\n\t}\r\n\telse if (text == "acctNum"){\r\n\t\tif (optinLang == "de_DE"){\r\n\t\t\twin = window.open(\'https://3dsec.postfinance.ch/acspage/de_DE_base_postfinance/info_acctNum.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "en_US"){\r\n\t\t\twin = window.open(\'en_US_base_postfinance/info_acctNum.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "fr_FR"){\r\n\t\t\twin = window.open(\'fr_FR_base_postfinance/info_acctNum.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}else if (optinLang == "it_IT"){\r\n\t\t\twin = window.open(\'it_IT_base_postfinance/info_acctNum.html\',"Help","height=100,width=400,dependent=yes,scrollbars=yes,resizable=yes,screenX=650,screenY=350,left=650,top=350");\r\n\t\t}\r\n\t}\r\n}\r\n\r\nfunction onBeforeUnloadHandler(object)\r\n{\r\n\tif ( !refreshing && closing )\r\n\t{\r\n\t\tevent.returnValue = "Your purchase has not completed!\\n To complete your purchase click \'Sp\xc3\xa4ter registrieren\' or \'Weiter\'.";\r\n\t}\r\n}\r\n\r\nfunction onFocusHandler()\r\n{\r\n\tclosing = true;\r\n}\r\n\r\nfunction OnLocaleChange(objLocale)\r\n{\r\n\tclosing = false;\r\n\t// save the current card holder name\r\n\tvar nLocale = objLocale.selectedIndex;\r\n\tif (nLocale >= 0)\r\n\t\tdocument.optInForm.ChangeLocale.value = objLocale.options[nLocale].value;\r\n\tdocument.optInForm.submit();\r\n\treturn false;\r\n}\r\n\r\n\r\n// Configurable parameters:\r\n\r\nvar loadError = null;\r\n\r\nfunction objError (evnt)\r\n{\r\n\tloadError = "error";\r\n}\r\n\r\nfunction validateNotEmpty( strValue ) {\r\n   var strTemp = strValue;\r\n   strTemp = trimAll(strTemp);\r\n   if(strTemp.length > 0){\r\n     return true;\r\n   }\r\n   return false;\r\n}\r\n\r\nfunction trimAll( strValue ) {\r\nvar objRegExp = /^(\\s*)$/;\r\n\r\n    //check for all spaces\r\n    if(objRegExp.test(strValue)) {\r\n       strValue = strValue.replace(objRegExp, \'\');\r\n       if( strValue.length == 0)\r\n          return strValue;\r\n    }\r\n\r\n   //check for leading & trailing spaces\r\n   objRegExp = /^(\\s*)([\\W\\w]*)(\\b\\s*$)/;\r\n   if(objRegExp.test(strValue)) {\r\n       //remove leading and trailing whitespace characters\r\n       strValue = strValue.replace(objRegExp, \'$2\');\r\n    }\r\n  return strValue;\r\n}\r\n\r\nfunction OnUserInput(userInput)\r\n{\r\n\r\n\t//alert("OnUserInput");\r\n\r\n\tclosing = false;\r\n\tdocument.optInForm.optIn.value = userInput;\r\n\r\n\tvar dob = document.optInForm.dateofbirth1.value+"/"+document.optInForm.dateofbirth2.value+"/"+document.optInForm.dateofbirth3.value;\r\n\r\n\tvar dob_day = document.optInForm.dateofbirth1.value;\r\n\tvar dob_month = document.optInForm.dateofbirth2.value;\r\n\tvar dob_year = document.optInForm.dateofbirth3.value;\r\n\r\n\t//alert("dob - "+dob);\r\n\r\n\tvar expirydt = document.optInForm.expirydate1.value + "/" + document.optInForm.expirydate2.value;\r\n\r\n\t//alert("exp date - "+expirydt);\r\n\r\n\tvar expirydt_month = document.optInForm.expirydate1.value;\r\n\tvar expirydt_yr = document.optInForm.expirydate2.value;\r\n\r\n\tvar cvv2Val = document.optInForm.cvv2.value;\r\n\tvar passwortval = document.optInForm.Passwort.value;\r\n    //var accountNumber = document.optInForm.acctnumber1.value + document.optInForm.acctnumber2.value + document.optInForm.acctnumber3.value + document.optInForm.acctnumber4.value;\r\n\r\n\tif (userInput == 1){\r\n\t\tif (!(1200 >= 1200 && 1200 <= 1202 ) && (!document.optInForm.accept.checked)){\r\n\t\t\t\tif (optinLang == "en_US"){\r\n\r\n\t\t\t\t\tdocument.getElementById(\'error1\').innerHTML = \'<table class = "msg"><tr><td colspan="2" style="vertical-align;"> Please accept the provisions for using 3-D Secure and protect yourself from potential credit card misuse!</td></tr></table>\';\r\n\r\n\t\t\t\t} else if (optinLang == "fr_FR"){\r\n\r\n\t\t\t\t\tdocument.getElementById(\'error1\').innerHTML = \'<table class = "msg"><tr><td td colspan="2" style="vertical-align;"> Veuillez accepter les dispositions relatives &#225; l\\\'utilisation de 3-D Secure et prot&#233;gez-vous contre de possibles abus avec la carte de cr&#233;dit!</td></tr></table>\';\r\n\r\n\t\t\t\t} else if (optinLang == "de_DE"){\r\n\r\n\t\t\t\t\tdocument.getElementById(\'error1\').innerHTML = \'<table class = "msg"><tr><td td colspan="2" style="vertical-align;"> Bitte akzeptieren Sie die Bestimmungen f&#252;r die Nutzung von 3-D Secure und sch&#252;tzen Sie sich vor m&#246;glichem Kreditkartenmissbrauch!</td></tr></table>\';\r\n\r\n\t\t\t\t} else if (optinLang == "it_IT"){\r\n\r\n\t\t\t\t\tdocument.getElementById(\'error1\').innerHTML = \'<table class = "msg"><tr><td td colspan="2" style="vertical-align;"> Accettate le disposizioni per l\\\'utilizzo di 3-D Secure e proteggetevi da un eventuale uso fraudolento della carta.</td></tr></table>\';\r\n\t\t\t\t} else {\r\n\r\n\t\t\t\t\tdocument.getElementById(\'error1\').innerHTML = \'<table class = "msg"><tr><td colspan="2" style="vertical-align;"> Bitte akzeptieren Sie die Bestimmungen f&#252;r die Nutzung von 3-D Secure und sch&#252;tzen Sie sich vor m&#246;glichem Kreditkartenmissbrauch!</td></tr></table>\';\r\n\t\t\t\t}\r\n\r\n\t\t\treturn false;\r\n\t\t}\r\n\r\n\t\tif (document.optInForm.CHname.value == "" || cvv2Val == "" || dob_day == "" || dob_month == "" || dob_year == "" || expirydt_month == "" || expirydt_yr == ""){\r\n\r\n\t\t\tif (optinLang == "en_US"){\r\n\t\t\t\tdocument.getElementById(\'error1\').innerHTML = \'<table class = "msg"><tr><td colspan="2"  style="vertical-align;"> Please complete all fields.</td></tr></table>\';\r\n\r\n\t\t\t} else if (optinLang == "fr_FR"){\r\n\t\t\t\tdocument.getElementById(\'error1\').innerHTML = \'<table class = "msg"><tr><td colspan="2"  style="vertical-align;"> Veuillez remplir tous les champs.</td></tr></table>\';\r\n\r\n\t\t\t} else if (optinLang == "de_DE"){\r\n\t