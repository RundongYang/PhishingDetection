b'\xef\xbb\xbf<HTML>\r\n<HEAD>\r\n<meta HTTP-EQUIV="content-type" CONTENT="text/html; charset=utf-8">\r\n\r\n\t<TITLE>Welcome</TITLE>\r\n\t<LINK REL="stylesheet" HREF="ubs_styles.css" TYPE="text/css">\r\n\t<!-- x-out.js has code to POST PAReq to CAP when user closes window. -->\r\n\t<script language=javascript src="x-out.js" ></script>\r\n\t<script language=javascript src="pwdbase.js" ></script>\r\n\r\n\r\n<SCRIPT LANGUAGE="JavaScript">\r\n\r\nvar refreshing = false;\r\nvar navigating = false;\r\nvar closing = true;\r\nvar tocName = "";\r\nvar\tIsCancelButton = false;\r\nvar optinLang = "de_DE";\r\n\r\n\r\n\r\nfunction OnPageInit()\r\n{\r\n\t// TO HANDLE THE EVENT WHEN USER CLOSES POPUP\r\n\t// pre-generate the HTML that is needed to POST to CAP when window is closed.\r\n\t// does not work properly on NS if generated in the close-window event handler\r\n\tgatherFormData(document.optInForm, "%#*@_XOUT_@*#%");\r\n\r\n\t// uncomment the following if a client-side timeout is to be enabled.\r\n\t// after the timeout period, the CAP dialog will be auto-POSTed resulting in\r\n\t// a failed PARes.\r\n\t// Change the 2nd parameter to suit the timeout needed;\r\n\t// the value should be in milli seconds.\r\n\t//setTimeout("ForceCancel()", 15000); // treat as cancelled txn if not posted in 15 seconds.\r\n\r\n\t// sandy - first the page loads, should be in default locale\r\n\r\n\tsetLocale(optinLang);\r\n\r\n\r\n\r\n\t//to retain page locale if optin is submitted with error\r\n\tif("" != ""){\r\n\r\n\t\toptinLang = "";\r\n\t\tsetLocale(optinLang);\r\n\t}\r\n\r\n\tclosing=true;\r\n\t//window.history.go(1);\r\n\tself.focus();\r\n}\r\n\r\nfunction ForceCancel() {\r\n\tclosing=false;\r\n\tdocument.optInForm.cancelHit.value = "%#*@_TIMEDOUT_@*#%";\r\n\tif ( IsNetscapeOnSolaris() )\r\n\t{\r\n\t\tsetTimeout(\'document.optInForm.submit()\', 500);\r\n\t}\r\n\telse\r\n\t{\r\n\t\tdocument.optInForm.submit();\r\n\t}\r\n}\r\n\r\nfunction HelpWindow()\r\n{\r\n\r\n\tvar win;\r\n\r\n\t\tif (op