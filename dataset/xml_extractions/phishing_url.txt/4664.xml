abel %>: <%= result.id %></strong>\r\n\r\n                        <% if (!isBlank(result.signature)) { %>\r\n                        <span>\r\n                            <% if (!isBlank(result.signature.label)) { %>\r\n                            <%= result.signature.label %>:\r\n                            <% } %>\r\n                            <%= result.signature.signatory %>\r\n                        </span>\r\n                            <% if (!isBlank(result.signature.link, \'url\')) { %>\r\n                        <span class="<%= result.signature.type == \'epod\' ? \'result-signature-epod\' : \'result-signature-proview\' %>"><a class="arrow" target="_blank" href="http://dhl.com<%= result.signature.link.url %>"><%= result.signature.link.label %></a></span>\r\n                            <% } %>\r\n                        <% } %>\r\n                    </td>\r\n                    <td>\r\n                        <% if (!isBlank(result.signature)) { %>\r\n                        <span><%= result.signature.description %></span>\r\n                        <% } %>\r\n\r\n                        <% if (!isBlank(result.origin)) { %>\r\n                        <span><%= result.origin.label %>:</span>\r\n                            <% if (!isBlank(result.origin.url)) { %>\r\n                        <a class="arrow" target="_blank" href="http://dhl.com<%= result.origin.url %>"><%= result.origin.value %></a>\r\n                            <% } else { %>\r\n                        <%= result.origin.value %>\r\n                            <% } %>\r\n                        <% } %>\r\n\r\n                        <% if (isBlank(result.eventRemark) && !isBlank(result.destination)) { %>\r\n                        <span><%= result.destination.label %>:</span>\r\n                            <% if (!isBlank(result.destination.url)) { %>\r\n                        <a class="arrow" target="_blank" href="http://dhl.com<%= result.destination.url %>"><%= result.destination.value %