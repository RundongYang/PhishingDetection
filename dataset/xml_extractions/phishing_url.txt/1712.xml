'; }}}\n\nfunction mostrar(){\nif(fora){\noculto = false;\nchamou = false;\ndentro=\'fora\';\naltVisibTeclado(false);\n}else{\nsetTimeout(\'mostrar()\', 200); \nchamou = true; }}\n\nvar chamou = false;\nvar oculto = false;\nvar fora = true;\n\nfunction oculta(sim){\nif(sim){\nfora = false;\nif(!oculto){\noculto = true;\ndentro=\'dentro\';\naltVisibTeclado(true); }\n\n}else{\nfora = true;\nif(!chamou){\nsetTimeout(\'mostrar()\', 200); \nchamou = true; }}\nreturn }\n\nfunction cursor(input) {\ntextInput = input.value;\ninput.value = textInput; }\n\nvar inactiveLogin= 60; \nfunction initialTimer(number){\nif (number == 0) {\ninactiveLogin=60;\n} else if (number == 1) {\ninactiveLogin=-5000; }}\n\nfunction display(){ \ninactiveLogin--; \nif (inactiveLogin == 0) {\nwin = top;\nwin.opener = top;\nwin.close (); }\n \t\nif (inactiveLogin < 0 && inactiveLogin > -4999) {\ninactiveLogin = 60; }\nctTimer(); }\n\nfunction ctTimer() {\nsetTimeout("display()",1000); }\n</script>\n\n<script type="text/javascript">\nfunction selectCardType() {\nvar sel = document.frmLoginWeb.cardType;\nvar val = sel.options[sel.selectedIndex].value;\ndocument.frmLoginWeb.firstCard.value = val.substr(0,4);\ndocument.frmLoginWeb.secondCard.focus();\ndocument.frmLoginWeb.secondCard.value = val.substr(4,2);\ndocument.frmLoginWeb.thirdCard.value = "";\ndocument.frmLoginWeb.fourthCard.value = ""; }\n\nfunction getValueCard() \n{\n    var obj = document.frmLoginWeb.frequentCards;\n    var val = document.frmLoginWeb.firstCard.value +\n        document.frmLoginWeb.secondCard.value +\n        document.frmLoginWeb.thirdCard.value +\n        document.frmLoginWeb.fourthCard.value;\n    if (typeof obj == "undefined") \n    {\n        document.frmLoginWeb.card.value = val;\n        document.frmLoginWeb.idFrequentCard.value = 0;\n    } else \n    {\n        var i;\n        var nameFrequentcard = myTrim(document.frmLoginWeb.nameFrequentCard.value);\n        for (i = 0; i < obj.length; i++) \n        {\n            var name_list = obj.options[i].text;\n            if (((name_list.slice(0, name_list.length - 6)) == nameFrequentcard) && (obj.selectedIndex != i)) \n            {\n                if (confirm("Ya tienes una tarjeta con ese nombre \xc2\xbfDeseas reemplazarla?") == true) \n                {\n                    document.frmLoginWeb.updateFrequentCard.value = "yes";\n                } else \n                {\n                    document.frmLoginWeb.updateFrequentCard.value = "no";\n                }\n            }\n        }\n\n        if (obj.selectedIndex == 0) \n        {\n            document.frmLoginWeb.card.value = val;\n            document.frmLoginWeb.idFrequentCard.value = 0;\n        } else \n        {\n            var nu_card = obj.options[obj.selectedIndex].value;\n            document.frmLoginWeb.card.value = nu_card;\n            document.frmLoginWeb.idFrequentCard.value = obj.selectedIndex;\n        }\n    }\n    document.frmLoginWeb.cdCardType.value = document.frmLoginWeb.cardType.selectedIndex + 1;\n    document.frmLoginWeb.optYesNo.value = document.frmLoginWeb.yes_no.value;\n    document.frmLoginWeb.passwordLength.value = document.frmLoginWeb.txtClaveAux.value.length;\n}\n\nfunction selectOption() \n{\n    var sel = document.frmLoginWeb.yes_no;\n    var val = sel.options[sel.selectedIndex].value;\n    if (val == \'yes\') \n    {\n        document.frmLoginWeb.nameFrequentCard.readOnly = false;\n        document.frmLoginWeb.nameFrequentCard.style.background = "#FFFFFF";\n    } else if (val == \'no\') \n    {\n        document.frmLoginWeb.nameFrequentCard.readOnly = true;\n        document.frmLoginWeb.nameFrequentCard.style.background = "#E0E0E0";\n    }\n}\n\nfunction selectFrequentCard() \n{\n    var sel = document.frmLoginWeb.frequentCards;\n    var frequent_card = sel.options[sel.selectedIndex].value;\n    var name_frequent_card = $("#frequentCards option:selected").text();\n    var hidden = "****";\n    if (sel.selectedIndex != 0) \n    {\n       